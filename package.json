{
  "name": "stars-schema",
  "version": "1.0.0",
  "author": "Ichinga Samuel",
  "license": "MIT",
  "description": "A mongoose schema plugin for implementing a stars rating system",
  "private": false,
  "types": "dist/cjs/types/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && fixup.sh",
    "prepack": "npm run build",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ichinga-Samuel/stars-schema.git"
  },
  "bugs": {
    "url": "https://github.com/Ichinga-Samuel/stars-schema/issues"
  },
  "homepage": "https://github.com/Ichinga-Samuel/stars-schema#readme",
  "keywords": [
    "mongoose",
    "mongodb",
    "stars",
    "rating",
    "schema",
    "plugins"
  ],
  "dependencies": {
    "mongoose": "^5.13.5"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "coveralls": "^3.1.1",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
